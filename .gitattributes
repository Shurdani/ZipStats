# Auto detect text files and perform LF normalization
* text=auto

# Kotlin files
*.kt text eol=lf
*.kts text eol=lf

# Gradle files
*.gradle text eol=lf
*.properties text eol=lf
gradlew text eol=lf
*.bat text eol=crlf

# XML files
*.xml text eol=lf

# JSON files
*.json text eol=lf

# Markdown files
*.md text eol=lf

# Binary files
*.png binary
*.jpg binary
*.jpeg binary
*.webp binary
*.jar binary
*.apk binary
*.aab binary

